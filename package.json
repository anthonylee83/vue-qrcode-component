  "author": "Gerard Reches <gerardreches@gmail.com> (https://www.gerardreches.com)",
  "url": "https://github.com/gerardreches/vue-qrcode-component/issues",
  "email": "gerardreches@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/gerardreches/vue-qrcode-component.git"
  },
  "license": "MIT",
  "dependencies": {
    "vue": "^2.0.1",
    "@keeex/qrcodejs-kx": "^1.0.2"
  },
  "browserify": {
